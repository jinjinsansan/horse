      "lib/*": [
        "dist/lib/*"
      ],
      "automation": [
        "dist/automation/index.d.ts"
      ],
      "automation/*": [
        "dist/automation/*"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "playwright": "^1.56.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0"
  }
}
