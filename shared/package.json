{
  "name": "@horsebet/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for HorseBet",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "exports": {
    "./types/*": "./dist/types/*",
    "./lib/*": "./dist/lib/*",
    "./automation": {
      "types": "./dist/automation/index.d.ts",
      "default": "./dist/automation/index.js"
    },
    "./automation/*": "./dist/automation/*"
  },
